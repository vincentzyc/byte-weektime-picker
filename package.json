{
  "name": "byte-weektime-picker",
  "version": "1.1.1",
  "private": false,
  "description": "二进制格式的多段时间选择器，主要用于广告营销时段选择",
  "author": "vincentzyc",
  "keywords": [
    "vue",
    "component",
    "time-picker",
    "weektime",
    "bytetime",
    "weektime-picker",
    "bytetime-picker",
    "byte-weektime-picker"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentzyc/byte-weektime-picker"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-lib": "vue-cli-service build --target lib --name byteWeektime --dest dist ./src/index.js"
  },
  "main": "dist/byteWeektime.umd.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "dependencies": {
    "core-js": "^3.8.3",
    "element-ui": "^2.12.0",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "node-sass": "^8.0.0",
    "sass-loader": "^13.2.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-console": "off",
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
